{
  "name": "@inficen/eslint-plugin-prefer-types",
  "version": "0.0.5",
  "description": "prefer Typescript types over interfaces",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Lionel Tay",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "something": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s",
    "bump": "npm version $(conventional-recommended-bump -p conventionalcommits)",
    "clean": "rimraf dist",
    "build": "yarn clean && babel src --out-dir dist -x .ts --ignore **/*.test.ts",
    "build:old": "yarn clean && tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.7.13",
    "@types/requireindex": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@typescript-eslint/utils": "^5.35.1",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.3",
    "conventional-recommended-bump": "^6.1.0",
    "eslint": "^8.19.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "license": "ISC"
}
